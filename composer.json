{
    "name": "tomasvotruba/phpunit-json-result-printer",
    "description": "Print your PHPUnit result in JSON, so other tool can pipe it",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "require-dev": {
        "tracy/tracy": "^2.10",
        "rector/rector": "^0.19.2",
        "symplify/easy-coding-standard": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "TomasVotruba\\PHPUnitJsonResultPrinter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomasVotruba\\PHPUnitJsonResultPrinter\\Test\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpstan": "vendor/bin/phpstan"
    }
}
